{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "react/jsx-filename-extension": 0
    }
}
